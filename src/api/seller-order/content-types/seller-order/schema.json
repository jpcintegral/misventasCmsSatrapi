{
  "kind": "collectionType",
  "collectionName": "seller_orders",
  "info": {
    "singularName": "seller-order",
    "pluralName": "seller-orders",
    "displayName": "seller-order"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "codigo": {
      "type": "string",
      "unique": true
    },
    "fecha": {
      "type": "datetime"
    },
    "status": {
      "type": "enumeration",
      "default": "PENDIENTE",
      "enum": [
        "PENDIENTE",
        "PARCIAL",
        "PAGADO",
        "CANCELADO"
      ]
    },
    "total": {
      "type": "decimal"
    },
    "vendedor": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::vendedor.vendedor",
      "inversedBy": "seller_orders"
    },
    "items": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::seller-order-item.seller-order-item",
      "mappedBy": "seller_order"
    },
    "payments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::payment.payment",
      "mappedBy": "seller_order"
    },
    "comentario": {
      "type": "text"
    }
  }
}
